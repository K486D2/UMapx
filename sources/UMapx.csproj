<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <Version>4.0.4.1</Version>
    <PackageId>UMapx</PackageId>
    <Company>Valery Asiryan</Company>
    <Authors>Valery Asiryan</Authors>
    <Product>UMapx</Product>
    <Copyright>Valery Asiryan, Â© 2015-2021</Copyright>
    <Description>Digital signal processing library for .NET Core and .NET Framework. Fast and optimized.</Description>
    <PackageProjectUrl>https://github.com/asiryan/UMapx.NET</PackageProjectUrl>
    <RepositoryUrl>https://github.com/asiryan/UMapx.NET</RepositoryUrl>
    <RepositoryType>Open-source</RepositoryType>
    <PackageTags>image video signal processing analytics streaming wavelet window analysis matrix decomposition transformation complex algebra</PackageTags>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageIcon>umapxnet.png</PackageIcon>
    <AssemblyVersion>4.0.4.1</AssemblyVersion>
    <FileVersion>4.0.4.1</FileVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Optimize>true</Optimize>
    <DocumentationFile>bin\Debug\netstandard2.0\UMapx.xml</DocumentationFile>
    <OutputPath>bin\Debug\</OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Audio\ComplexFilters\**" />
    <Compile Remove="Audio\Features\**" />
    <Compile Remove="Audio\Filters\**" />
    <Compile Remove="Audio\Gui\**" />
    <Compile Remove="Audio\Signals\**" />
    <Compile Remove="Audio\Windows\**" />
    <EmbeddedResource Remove="Audio\ComplexFilters\**" />
    <EmbeddedResource Remove="Audio\Features\**" />
    <EmbeddedResource Remove="Audio\Filters\**" />
    <EmbeddedResource Remove="Audio\Gui\**" />
    <EmbeddedResource Remove="Audio\Signals\**" />
    <EmbeddedResource Remove="Audio\Windows\**" />
    <None Remove="Audio\ComplexFilters\**" />
    <None Remove="Audio\Features\**" />
    <None Remove="Audio\Filters\**" />
    <None Remove="Audio\Gui\**" />
    <None Remove="Audio\Signals\**" />
    <None Remove="Audio\Windows\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="Transform\BoxFilterOptions.cs" />
    <Compile Remove="Vision\Motion\BlobCountingObjectsProcessing.cs" />
    <Compile Remove="Vision\Motion\CustomFrameDifferenceDetector.cs" />
    <Compile Remove="Vision\Motion\GridMotionAreaProcessing.cs" />
    <Compile Remove="Vision\Motion\MotionBorderHighlighting.cs" />
    <Compile Remove="Vision\Motion\SimpleBackgroundModelingDetector.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="4.7.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="umapxnet.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
