<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <Version>4.0.2.3</Version>
    <PackageId>UMapx.NET</PackageId>
    <Company>Valery Asiryan</Company>
    <Authors>Valery Asiryan</Authors>
    <Product>UMapx.NET</Product>
    <Copyright>Valery Asiryan, Â© 2015-2020</Copyright>
    <Description>Digital signal processing library.</Description>
    <PackageProjectUrl>https://github.com/asiryan/UMapx.NET</PackageProjectUrl>
    <RepositoryUrl>https://github.com/asiryan/UMapx.NET</RepositoryUrl>
    <RepositoryType>Open-source</RepositoryType>
    <PackageTags>signal-processing analysis statistics wavelets transformations imaging matrix linear-algebra</PackageTags>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageIcon>umapxnet.png</PackageIcon>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Optimize>true</Optimize>
    <DocumentationFile>bin\Debug\netstandard2.0\UMapx.xml</DocumentationFile>
    <OutputPath>bin\Debug\</OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="4.7.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="umapxnet.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
